[M]
mogrify (terminal) : resize ukuran multiple foto
->mogrify IMG_12.jpg -resize 10% IMG_12.jpg

[N]
+ npm : 
# (install global) : nodemon, eslint 
# tambahkan script untuk menjalankan otomatis nodemon dan eslint secara bersamaan: 
"scripts": {
    "start": "nodemon app.js --exec 'npm run lint && node'",
    "lint": "eslint app.js --fix"
}

